<opt>
     <!-- Include airtightness (1) or not (0)-->
     <INC_AIRTIGHT>1</INC_AIRTIGHT>
     <!-- Ceiling upgrade assume that blown in insulation is just placed on top of existing insulation-->
     <CEIL_INS ins_sys="0">
         <sys_1 max_RSI="7.04" ins_k="0.043" ins_rho="16.0" ins_Cp="835.0"/>
         <sys_2 max_RSI="10.57" ins_k="0.043" ins_rho="16.0" ins_Cp="835.0"/>
         <sys_3 max_RSI="14.09" ins_k="0.043" ins_rho="16.0" ins_Cp="835.0"/>
     </CEIL_INS>
     <!-- Ventilation system type. vent_sys=0 will yield just a fan, 1 is HRV, 2 is ERV-->
