         </HRV>
         <!--ERV>
             <flowrate>22</flowrate>
             <Supply_22 detail="Venmar 50H"    hsp_T="0.00" hsp_SRE="66.0" hsp_LRMT="0.45" hsp_P="39.00" vltt_T="-25.00" vltt_SRE="60.0" hsp_LRMT="0.45" vltt_LRMT="0.26" vltt_P="48.00"  tre="40.0" Preheat_P="0"/>
         </ERV-->
     </VNT>
     <!-- Wall upgrade assumes that new insulation is added to the exterior (full height)-->
     <WALL_INS RSI_goal="2" clad="1" ins="1">
         <goal_1>4.23</goal_1> <!-- R24 -->
         <goal_2>4.75</goal_2> <!-- R27 -->
         <goal_3>5.46</goal_3> <!-- R31 -->
         <!-- ins_k: Conductivity (W/mK), ins_rho: Density (kg/m3), ins_Cp: Specific Heat (J/kgK), ins_t: Thickness (m)-->
         <clad_1 cld_k="0.16" cld_rho="1380" cld_Cp="1000" cld_t="0.003" description="siding_UPG - Vinyl"/>
         <!-- ins_k: Conductivity (W/mK), ins_rho: Density (kg/m3), ins_Cp: Specific Heat (J/kgK), n_t: number of different board thicknesses-->
         <ins_1 ins_k="0.043" ins_rho="16.0" ins_Cp="835.0" description="insulation_UPG - XPS" n_t="3">
            <!--Thicknesses (m), low to high -->
            <t_1>0.0254</t_1> <!--One inch -->
            <t_2>0.0381</t_2> <!--One and half inch -->
            <t_3>0.0508</t_3> <!--Two inch -->
         </ins_1>
     </WALL_INS>
     <!-- Foundation upgrade assumes that new insulation is added to the interior (full height)-->
     <BASE_INS ins_sys="2">
         <sys_1>
             <bsmt max_RSI="1.76"/>
             <!-- EPS: (ASHRAE Fundamentals 2009)-->
             <!--crawl max_RSI="3.17" ins_k="0.036" ins_rho="20.0" ins_Cp="1470.0"/-->
             <!-- XPS (ASHRAE Fundamentals 2009)-->
             <crawl max_RSI="5.3" ins_k="0.026" ins_rho="33.0" ins_Cp="1470.0"/>
         </sys_1>
         <sys_2>
