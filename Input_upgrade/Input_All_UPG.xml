         <HRV>
             <flowrate>22</flowrate>
             <flowrate>51</flowrate>
             <flowrate>39</flowrate>
             <flowrate>31</flowrate>
             <Supply_22 detail="Venmar 50H"    hsp_T="0.00" hsp_SRE="66.0" hsp_P="39.00" vltt_T="-25.00" vltt_SRE="60.0" vltt_P="48.00"  tre="40.0" Preheat_P="0"/>
             <Supply_31 detail="Venmar 90HV+"  hsp_T="0.00" hsp_SRE="75.0" hsp_P="54.00" vltt_T="-25.00" vltt_SRE="70.0" vltt_P="72.00"  tre="40.0" Preheat_P="0"/>
             <Supply_39 detail="vanEE 200H"    hsp_T="0.00" hsp_SRE="65.0" hsp_P="71.00" vltt_T="-25.00" vltt_SRE="60.0" vltt_P="114.00" tre="40.0" Preheat_P="0"/>
